    private void cbMethodsActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_cbMethodsActionPerformed
        // TODO add your handling code here:
        String selectedCategory = (String) cbMethods.getSelectedItem();
        System.out.println("Selected Category: " + selectedCategory); 
    // Clear the second combo box
        cbShops.removeAllItems();

        // Add items to the second combo box based on the selection
        if ("Deposit Machines".equals(selectedCategory)) {
            cbShops.addItem("eTap");
            cbShops.addItem("TouchPay");
        } else if ("Conveniences Stores".equals(selectedCategory)) {
            cbShops.addItem("7-Eleven");
            cbShops.addItem("AlfaMart");
            cbShops.addItem("Uncle John's");
        } else if ("Pawnshops".equals(selectedCategory)) {
            cbShops.addItem("Cebuana");
            cbShops.addItem("M Lhuillier");
            cbShops.addItem("Palawan");
        } else if ("Sari-Sari Stores".equals(selectedCategory)) {
            cbShops.addItem("GCash Pera Outlet");
        }
    }//GEN-LAST:event_cbMethodsActionPerformed

